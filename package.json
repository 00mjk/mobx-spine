{
  "name": "mobx-spine",
  "version": "0.2.1",
  "license": "ISC",
  "author": "Kees Kluskens <kees@webduck.nl>",
  "description": "MobX with support for models, relations and an API.",
  "browser": "dist/mobx-spine.es.js",
  "main": "dist/mobx-spine.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/CodeYellowBV/mobx-spine.git"
  },
  "scripts": {
    "test": "jest",
    "test-dev": "jest --watch",
    "test-coverage": "jest --coverage",
    "lint": "eslint src",
    "preversion": "npm run -s lint",
    "build": "rm -f dist/** && node build.js",
    "ci": "npm run -s lint && npm run -s test-coverage && codecov"
  },
  "dependencies": {},
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "axios": "^0.15.3",
    "axios-mock-adapter": "^1.7.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "codecov": "^1.0.1",
    "eslint": "^3.14.1",
    "eslint-config-codeyellow": "^3.0.2",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "mobx": "^3.0.2",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1"
  },
  "peerDependencies": {
    "axios": "^0.15.3",
    "lodash": "^4.17.4",
    "mobx": "^3.0.2"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "/fixtures/"
    ]
  }
}
